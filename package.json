{
  "name": "account",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "rm -rf dist && tsc",
    "prestart": "npm run build",
    "dev": "nodemon --exec ts-node ./src/index.ts --mode development",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pretest": "cross-env TEMPLATE_NODE_ENV=test npm run migrate-test:reset && npm run migrate-test:up",
    "test": "cross-env TEMPLATE_NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --require ts-node/register --exit --recursive --timeout=150000 tests/index.ts",
    "prepare": "husky install",
    "migrations": "source .env && db-migrate",
    "migrate:up": "db-migrate up  --config database.json",
    "migrate:down": "db-migrate down  --config database.json",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "migrate:reset": "db-migrate reset --config database.json",
    "migrate-test:reset": "db-migrate reset --config database.json -e test",
    "seed:create": "db-migrate create:seed --config database.json",
    "seed:up": "db-migrate up:seed --config database.json",
    "seed:down": "db-migrate down:seed --config database.json",
    "seed:reset": "db-migrate reset:seed --config database.json",
    "seed:reset:test": "db-migrate reset:seed --config database.json -e test",
    "seed:up:test": "db-migrate up:seed --config database.json -e test"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "chai-http": "^4.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "file-stream-rotator": "^1.0.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.4.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "pg-promise": "^11.5.4",
    "sinon": "^15.2.0",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.12.12",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
